<div class="section head">
  <div class="corset">
    <h1>Glyphicons (from twitter bootstrap)</h1>
  </div>
</div>

<div class="section hard-top">
  <div class="corset">
    
    <% for icon, i in glyphicons: %>
    
      <div class="icon_sample">
				<div class="sample_icon">
					<i class="glyphicon glyphicon-<%= icon %>"></i>
				</div>
				<div class="sample_name">
					<%= icon %>
				</div>
			</div>
    
    <% end%>
    
  </div>
</div>